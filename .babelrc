{
  "assumptions": { "setPublicClassFields": true },
  "presets": ["@babel/preset-typescript"],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "decoratorsBeforeExport": true
      }
    ],
    "babel-plugin-add-import-extension",
    "@babel/plugin-proposal-class-properties"
  ]
}
